{
  "name": "@migerh/js-module-walker",
  "version": "0.3.0-unstable",
  "description": "Analyze and understand dependencies in js projects",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "./tests.sh"
  },
  "bin": {
    "js-module-walker": "./index.js"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "source": [
      "index.js",
      "lib/**/*.js"
    ],
    "concurreny": 5,
    "verbose": true
  },
  "nyc": {
    "include": [
      "lib/**/*.js",
      "index.js"
    ],
    "exclude": [
      "tests/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "check-coverage": false,
    "report-dir": "./coverage"
  },
  "bugs": {
    "url": "https://github.com/migerh/js-module-walker/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/migerh/js-module-walker.git"
  },
  "author": "Michael Gerhaeuser <michael.gerhaeuser@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.5.5",
    "commander": "^3.0.0",
    "lodash": "^4.17.21",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1",
    "stream-buffers": "^3.0.2",
    "uuid": "^3.3.2"
  }
}
